{
  "name": "sunnyq",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "format": "prettier --ignore-path .gitignore --write **/*.{js,jsx,ts,tsx,json,css}",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "check-format": "prettier --ignore-path .gitignore --check **/*.{js,jsx,ts,tsx,json,css}",
    "check-all": "yarn check-format && yarn check-types && yarn lint"
  },
  "dependencies": {
    "eslint-config-prettier": "^9.1.0",
    "lint-staged": "^15.2.2",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^8.0.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
